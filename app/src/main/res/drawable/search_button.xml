    <corners android:radius="24dp" />
    <gradient
        android:angle="45"

        android:centerColor="#23d2f8"
        android:centerX="35%"
        android:endColor="#23d2f8"
        android:startColor="#23d2f8"
        android:type="linear" />
    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" />
